{
  "name": "shawtyfy",
  "version": "1.0.0",
  "description": "simple light weight url shortener",
  "type": "module",
  "main": "server.js",
  "scripts": {
    "test": "jest",
    "start": "node server.js",
    "dev": "nodemon server.js"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(nanoid)/)"
    ]
  },
  "keywords": [
    "url",
    "shortener",
    "node"
  ],
  "author": "Sutirtha",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "mongodb": "^6.1.0",
    "mongoose": "^7.5.2",
    "nanoid": "^5.0.1",
    "nodemon": "^3.0.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-typescript": "^7.22.15",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3"
  }
}
